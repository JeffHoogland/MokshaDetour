			}												\
		}													\
		description {										\
			state: "visible" 0.0;							\
			inherit: "default" 0.0;							\
			visible: 1;										\
		}													\
	}														\
	part {													\
		name: "icon0";										\
		type: RECT;											\
		mouse_events: 0;									\
		description {										\
			state: "default" 0.0;							\
