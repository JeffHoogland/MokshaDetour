#define LIST_LABEL										\
	part {												\
		name: "e.text.label";							\
		type: TEXT;										\
		effect: TEXT_EFFECT;							\
		scale: 1;										\
		mouse_events: 0;								\
		description {									\
			state: "default" 0.0;						\
			min: 16 16;									\
			rel1 {										\
				to_x: "icon0";							\
				relative: 1.0 0.0;						\
