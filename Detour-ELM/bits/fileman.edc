			state: "default" 0.0;							\
			color: 255 255 255 221;							\
			visible: 0;										\
			rel1 {											\
				to: "icon_box";								\
				relative: 0.0 0.0;							\
				offset: -4 -4;								\
			}												\
			rel2 {											\
				to: "icon_box";								\
				relative: 1.0 1.0;							\
