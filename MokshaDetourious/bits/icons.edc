# ifdef ICON
#  undef ICON
# endif
#define ICON(GROUP_NAME,SIZE,IMAGE_NAME)			\
group {								\
	name: "e/icons/"GROUP_NAME;				\
	parts {							\
		part {						\
			name: "icon";				\
			type: IMAGE;				\
			repeat_events: 1;			\
			description {				\
				state: "default" 0.0;		\
				max: SIZE;			\
				aspect: 1.0 1.0; 		\
				aspect_preference: BOTH;	\
				image.normal: IMAGE_NAME;	\
			}					\
		}						\
	} 							\
}

