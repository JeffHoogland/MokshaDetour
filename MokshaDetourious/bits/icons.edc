

# ifdef ICON
#  undef ICON
# endif
#define ICON(NAME, IMAGE_NAME)					\
group { name: "e/icons/"NAME ;					\
	parts {							\
		part { 						\
			name: "icon";				\
			repeat_events: 1;			\
			description { 				\
				state: "default" 0.0;		\
				max: 24 24;			\
				aspect: 1.0 1.0;		\
				aspect_preference: BOTH;	\
				image { 			\
					normal: IMAGE_NAME ; 	\
				} 				\
			}					\
		}						\
	}							\
}

#define ICONMIME(NAME, IMAGE_NAME) ICON("mimetypes/"NAME, IMAGE_NAME)

