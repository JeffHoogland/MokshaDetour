# endif
#define ICON(GROUP_NAME,IMAGE_NAME) \
group { \
	name: "e/icons/enlightenment/"GROUP_NAME; \
	min: 24 24; \
	max: 24 24; \
	parts { \
		part { \
			name: "screen"; \
			type: IMAGE; \
			mouse_events:  0; \
			description { \
				min: 24 24; \
				max: 24 24; \
				fixed: 1 1; \
				state: "default" 0.0; \
				aspect: 1.0 1.0; \
				aspect_preference: BOTH; \
				image { \
					normal: IMAGE_NAME; \
				} \
			} \
		} \
	} \
}

