
group {
	name: "e/widgets/label";

	parts {
		part {
			name: "e.text.label";
			type: TEXT;
			effect: TEXT_EFFECT;
			scale: 1;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				rel1 { 
					relative: 0.0 0.0; 
					offset: 2 2; 
				}

				rel2 { 
					relative: 1.0 1.0; 
					offset: -3 -3; 
				}
				TEXT_COLOR;
				TEXT_NORMAL(0, 0.0 0.5, 1 1);
				text.ellipsis: -1;
			}
		}
	}
}

