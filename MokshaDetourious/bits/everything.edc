				inherit: "default" 0.0;
				visible: 1;
			}
		}

		part {
			name: "e.text.label";
			type: TEXT;
			clip_to: "base";

/* #if (BUILD_ILLUME == 0)
 * 			effect: TEXT_EFFECT_BOLD;
 * #endif */
			mouse_events: 0;
			scale: 1;
			description {
				state: "default" 0.0;
				align: 0.5 1.0;
				rel1.to: "base";
				rel2.to: "base";
				rel1.offset: 2 0;
				rel2.offset: -3 -1;
				rel1.relative: 0.0 1.0;

#if (EVRY_TRANSPARENT == 1)
				TEXT_COLOR_SELECTED;
#else
				TEXT_COLOR;
#endif
				TEXT_SMALL("label_normal", 0.5 0.5, 0 1);
			}
			description {
				state: "selected" 0.0;
				inherit: "default" 0.0;
				TEXT_COLOR_SELECTED;
			}
		}

		part {
			name: "event";
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 0;
			}
		}
	}
	programs {
		program {
			name: "sel";
			signal: "e,state,selected";
			source: "e";
			action: STATE_SET "selected" 0.0;

#if (BUILD_ILLUME == 1)
			transition: SINUSOIDAL 0.2;
#endif
			target: "bg";
			target: "e.text.label";
		}
		program {
			name: "unsel";
			signal: "e,state,unselected";
			source: "e";
			action: STATE_SET "default" 0.0;
			transition: ACCELERATE 0.1;
			target: "bg";
			target: "e.text.label";
		}
		program {
			name: "mark";
			signal: "e,state,marked";
			source: "e";
			action: STATE_SET "marked" 0.0;
			target: "bg_mark";
			transition: ACCELERATE 0.15;
		}
		program {
			name: "unmark";
			signal: "e,state,unmarked";
			source: "e";
			action: STATE_SET "default" 0.0;
			transition: LINEAR 0.2;
			target: "bg_mark";
		}
		program {
			name: "arrow_show";
			signal: "e,state,browseable";
			source: "e";
			action: STATE_SET "visible" 0.0;
			target: "arrow";
		}
		program {
			name: "thumb_show_delayed";
			signal: "e,action,thumb,show_delayed";
			source: "e";
			action: STATE_SET "visible" 0.0;
			target: "base";
		}
		program {
			name: "thumb_show";
			signal: "e,action,thumb,show";
			source: "e";
			action: STATE_SET "visible" 0.0;
			target: "base";
		}
		program {
            name: "go_big";
            action: STATE_SET "default" 0.0;
            target: "base_anim";
            after: "go_big2";
		}
		program {
            name: "go_big2";
            action: STATE_SET "hidden" 0.0;
            transition: DECELERATE 0.1;
            target: "base_anim";
            after: "go_big3";
		}
		program {
            name: "go_big3";
            action: STATE_SET "huge" 0.0;
            transition: DECELERATE 0.1;
            target: "base_anim";
            after: "go_big4";
		}
		program {
            name: "go_big4";
            action: STATE_SET "default" 0.0;
            transition: DECELERATE 0.1;
            target: "base_anim";
		}
		program {
            name: "action_go";
            signal: "e,action,go";
            source: "e";
            action: ACTION_STOP;
            target: "go_big";
            target: "go_big2";
            target: "go_big3";
            after: "go_big";
		}
	}
}
