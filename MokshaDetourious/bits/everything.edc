         part {
            name: "sb_vbar_over1";
            clip_to: "sb_vbar";
            mouse_events: 0;
            description {
               state: "default" 0.0;
               rel1.to: "e.dragable.vbar";
               rel2.relative: 1.0 0.5;
               rel2.to: "e.dragable.vbar";
               image {
                  normal: "images/bt_sm_hilight.png";
                  border: 6 6 6 0;
