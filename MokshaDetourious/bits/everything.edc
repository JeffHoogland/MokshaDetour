				}
				rel2 {
					relative: 0.0 1.0;
					offset: 4 -5;
					to_y: "base";
					to_x: "arrow";
				}
				TEXT_COLOR;
				TEXT_SMALL("label_small", 0.0 0.0, 0 1);
