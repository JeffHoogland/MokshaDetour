				TEXT_NORMAL(0, 0.0 0.0, 0 1);
			}
			description {
				state: "selected" 0.0;
				inherit: "default" 0.0;
				TEXT_COLOR_SELECTED;
			}
		}

		part {
			name: "e.text.detail";
			type: TEXT;
			scale: 1;
			clip_to: "base";
			mouse_events: 0;
			description {
				state: "default" 0.0;
				visible: 1;
				rel1 {
					to_x: "clip";
					to_y: "base";
					offset: 5 6 ;
