               color: 0 0 0 0;
               rel1 {
                  to: "sb_vbar";
                  relative: 0.0  0.0;
                  offset: 0    0;
               }
               rel2 {
                  to: "sb_vbar";
                  relative: 1.0  0.0;
                  offset: -1   0;
               }
            }
         }
         part {
            name: "sb_vbar_a2";
            type: RECT;
            clip_to: "sb_vbar";
            mouse_events: 1;
