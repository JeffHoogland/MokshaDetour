            clip_to: "sb_vbar";
            mouse_events: 1;
            description {
               state: "default" 0.0;
               color: 0 0 0 0;
               rel1 {
                  relative: 0.0 1.0;
                  to: "e.dragable.vbar";
               }
               rel2 {
                  relative: 1.0 0.0;
                  to: "sb_vbar_a2";
               }
            }
         }
         part {
            name: "e.dragable.vbar";
            clip_to: "sb_vbar";
            mouse_events: 1;
            scale: 1;
            dragable {
               x: 0 0 0;
               y: 1 1 0;
               confine: "sb_vbar_base";
            }
            description {
               state: "default" 0.0;
               min: 9 17;
               rel1 {
                  relative: 0.5  0.5;
                  offset: -1    0;
                  to: "sb_vbar_base";
               }
               rel2 {
                  relative: 0.5  0.5;
                  offset: -1    0;
                  to: "sb_vbar_base";
               }
               image {
                  normal: "images/bt_sm_base2.png";
                  border: 6 6 6 6;
