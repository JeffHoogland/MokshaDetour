               rel2.to: "sb_vbar_base";
               image {
                  normal: "images/sb_runnerv.png";
                  border: 0 0 4 4;
               }
               fill.smooth: 0;
            }
         }
         part {
            name: "sb_vbar_p1";
            type: RECT;
            clip_to: "sb_vbar";
            mouse_events: 1;
            description {
               state: "default" 0.0;
               color: 0 0 0 0;
               rel1 {
                  relative: 0.0 1.0;
                  to: "sb_vbar_a1";
               }
               rel2 {
                  relative: 1.0 0.0;
                  to: "e.dragable.vbar";
               }
            }
         }
         part {
            name: "sb_vbar_p2";
