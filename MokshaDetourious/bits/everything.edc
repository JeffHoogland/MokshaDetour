                  relative: 1.0  0.0;
                  offset: -1    0;
                  to: "sb_vbar_a2";
               }
            }
         }
         part {
            name: "sb_vbar_runner";
            clip_to: "sb_vbar";
            mouse_events: 0;
            description {
               state: "default" 0.0;
               max: 3 99999;
               rel1.to: "sb_vbar_base";
