		part {
			name: "e.text.label";
			type: TEXT;
			/* effect: TEXT_EFFECT; */
			scale: 1;
			clip_to: "base";
			mouse_events: 0;
			description {
				state: "default" 0.0;
				rel1 {
					to_x: "clip";
					to_y: "base";
					offset: 5 -5;
					relative: 1.0 0.0;
				}
				rel2 {
					relative: 0.0 1.0;
					offset: 4 -2;
					to_x: "arrow";
					to_y: "spacer";
				}
