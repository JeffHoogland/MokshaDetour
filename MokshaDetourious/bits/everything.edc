			target: "sb_vbar";
		}
		program {
			name: "sb_vbar_hide";
			signal: "e,action,hide,vbar";
			source: "e";
			action: STATE_SET "hidden" 0.0;
			target: "sb_vbar";
		}
		program {
			name: "sb_vbar_a1_down2";
