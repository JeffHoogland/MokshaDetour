
group {
	name: "e/transpreview/0";
	parts {

		part {
			name: "bg";
			type: IMAGE;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				image {
					normal: "images/wallpaper_pat0.png";
				}
				fill {
					size {
						relative: 0 0;
						offset: 240 240;
					}
				}
			}
		}
	}
}
group {
	name: "e/transpreview/1";
	parts {

		part {
			name: "bg";
			type: IMAGE;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				color: 255 255 255 128;
				image {
					normal: "images/wallpaper_pat0.png";
				}
				fill {
					size {
						relative: 0 0;
						offset: 240 240;
					}
				}
			}
		}
	}
	programs {
	}
}
group {
	name: "e/transitions/crossfade";

	parts {

		part {
			name: "e.swallow.bg.old";
			type: SWALLOW;
			description {
				state: "default" 0.0;
			}
		}

		part {
			name: "e.swallow.bg.new";
			type: SWALLOW;
			clip_to: "bg_new_clip";
			description {
				state: "default" 0.0;
			}
		}

		part {
			name: "bg_new_clip";
			type: RECT;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				color: 255 255 255 0;
			}
			description {
				state: "done" 0.0;
				inherit: "default" 0.0;
				color: 255 255 255 255;
			}
		}
	}
	programs {
		program {
			name: "go";
			signal: "e,action,start";
			source: "e";
			action: STATE_SET "done" 0.0;
			transition: SINUSOIDAL 1.0;
			target: "bg_new_clip";
			after: "go2";
		}
		program {
			name: "go2";
			action: SIGNAL_EMIT "e,state,done" "";
		}
	}
}
group {
	name: "e/transitions/vswipe";

	parts {

		part {
			name: "e.swallow.bg.old";
			type: SWALLOW;
			clip_to: "bg_prev_clip";
			description {
				state: "default" 0.0;
			}
		}

		part {
			name: "e.swallow.bg.new";
			type: SWALLOW;
			clip_to: "bg_new_clip";
			description {
				state: "default" 0.0;
			}
		}

		part {
			name: "bg_prev_clip";
			type: RECT;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				rel1 { 
					relative: 0.0 0.0; 
					offset: 0 -32; 
				}
				rel2 { 
					relative: 1.0 1.0; 
					offset: -1 31; 
				}
			}
			description {
				state: "done" 0.0;
				inherit: "default" 0.0;
				rel1 { 
					relative: 0.0 1.0; 
					offset: 0 31; 
				}
				rel2 { 
					relative: 1.0 1.0; 
					offset: -1 31; 
				}
			}
		}

		part {
			name: "bg_new_clip";
			type: RECT;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				rel1 {
					relative: 0.0 0.0;
					offset: 0 -32;
				}
				rel2 {
					relative: 1.0 0.0;
					offset: -1 -32;
				}
			}
			description {
				state: "done" 0.0;
				inherit: "default" 0.0;
				rel1 {
					relative: 0.0 0.0;
					offset: 0 -32;
				}
				rel2 {
					relative: 1.0 1.0;
					offset: -1 31;
				}
			}
		}

		part {
			name: "swipe_gap";
			type: IMAGE;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				min: 0 64;
				max: 99999 64;
				rel1 { 
					to: "bg_prev_clip"; 
					relative: 0.0 0.0; 
					offset: 0 0; 
				}
				rel2 { 
					to: "bg_prev_clip"; 
					relative: 1.0 0.0; 
					offset: -1 0; 
				}
				image {
					normal: "images/transition_im0.png";
				}
			}
		}
	}
	programs {
		program {
			name: "go";
			signal: "e,action,start";
			source: "e";
			action: STATE_SET "done" 0.0;
			transition: SINUSOIDAL 1.0;
			target: "bg_new_clip";
			target: "bg_prev_clip";
			after: "go2";
		}
		program {
			name: "go2";
			action: SIGNAL_EMIT "e,state,done" "";
		}
	}
}
