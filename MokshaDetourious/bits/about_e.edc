				}
			}
			description { 
				state: "visible" 0.0;
				inherit: "default" 0.0;
				color: 255 255 255 255;
			}
		}

		part { 
			name: "logo_clip";
			type: RECT;
			mouse_events: 0;
			description { 
				state: "default" 0.0;
				color: 255 255 255 255;
				rel1 { 
