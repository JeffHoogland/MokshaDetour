			type: TEXT;
			effect: SHADOW;
			scale: 1;
			mouse_events: 0;
			clip_to: "logo_clip";
			description { 
				state: "default" 0.0;
				color: 60 60 60 255;
				color3: 170 168 169 32;
				fixed: 1 1;
				rel1 { 
					to: "logo";
					relative: 0.5 1.0; 
					offset: 0 0; 
				}
				rel2 { 
					to: "logo"; 
					relative: 0.5 1.0; 
					offset: -1 16; 
				}
				TEXT_HEADER("label_title", 0.0 0.0, 1 1);
				text.ellipsis: -1;
			}
		}

		part { 
			name: "e.text.version";
			type: TEXT;
			effect: TEXT_EFFECT;
			scale: 1;
			mouse_events: 0;
			clip_to: "logo_clip";
			description { 
				state: "default" 0.0;
				color: 60 60 60 255;
				fixed: 1 1;
				rel1 { 
					to: "e.text.title"; 
					relative: 0.5 1.0; 
					offset: 0 0; 
				}
				rel2 { 
					to: "e.text.title"; 
					relative: 0.5 1.0; 
					offset: -1 10; 
				}
				TEXT_NORMAL(0, 1.0 0.5, 1 1);
				text.ellipsis: -1;
			}
		}

		part { 
			name: "small_logo";
			type: IMAGE;
			clip_to: "about_enlightenment_clip";
			description { 
				state: "default" 0.0;
