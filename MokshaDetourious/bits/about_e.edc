				}
			}
		}

		part { 
			name: "authors_entry";
			type: IMAGE;
			clip_to: "about_enlightenment_clip";
			description { 
				state: "default" 0.0;
				max: 9999 134;
				rel1 {
					to: "about_entry";
					relative: 0.0 1.0;
					offset: 0 0;
				}
				rel2 {
					to: "button.area";
					to_x: "base";
					relative: 1.0 0.0;
					offset: -5 -1;
				}
				image {
					normal: "images/entry_im0.png";
					border: 6 6 8 7;
				}
			}
		}

		part { 
			name: "about_clip";
			type: RECT;
			mouse_events: 0;
			clip_to: "about_enlightenment_clip";
			description { 
				state: "default" 0.0;
				color: 255 255 255 255;
				rel1 {
					to: "about_entry";
					relative: 0.0 0.0;
					offset: 6 3;
				}
				rel2 {
					to: "about_entry";
					relative: 1.0 1.0;
					offset: -7 -5;
				}
			}
		}

		part { 
			name: "authors_clip";
			type: RECT;
			mouse_events: 0;
			clip_to: "about_enlightenment_clip";
			description { 
				state: "default" 0.0;
				color: 255 255 255 255;
				rel1 {
					to: "authors_entry";
					relative: 0.0 0.0;
					offset: 6 3;
				}
				rel2 {
					to: "authors_entry";
					relative: 1.0 1.0;
					offset: -7 -5;
				}
			}
		}

		part { 
			name: "e.textblock.about";
			type: TEXTBLOCK;
			scale: 1;
			mouse_events: 0;
			clip_to: "about_clip";
			description { 
				state: "default" 0.0;
				align: 0.5 0.0;
				min: 160 0;
				max: 10000 10000;
				fixed: 1 1;
				rel1 {
					to: "about_entry";
					relative: 0.0 1.0;
					offset: 12 0;
				}
				rel2 {
					to: "about_entry";
					relative: 1.0 1.0;
					offset: -6 -1;
				}
				text {
					style: "about_enlightenment";
					min: 0 1;
				}
			}
			description { 
				state: "up" 0.0;
				inherit: "default" 0.0;
				align: 0.5 1.0;
				rel1 {
					relative: 0.0 0.0;
					offset: 12 0;
				}
				rel2 {
					relative: 1.0 0.0;
					offset: -6 -1;
				}
			}
		}

		part { 
			name: "e.textblock.authors";
			type: TEXTBLOCK;
			scale: 1;
			mouse_events: 0;
			clip_to: "authors_clip";
			description { 
				state: "default" 0.0;
				align: 0.5 0.0;
				min: 160 0;
				max: 10000 10000;
				fixed: 1 1;
				rel1 {
					to: "authors_entry";
					relative: 0.0 1.0;
					offset: 12 0;
				}
				rel2 {
					to: "authors_entry";
					relative: 1.0 1.0;
					offset: -6 0;
				}
				text {
					style: "about_enlightenment";
